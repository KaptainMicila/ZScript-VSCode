{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ZScript",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#types"
		},
		{
			"include": "#comment"
		},
		{
			"include": "#strings-doublequote"
		},
		{
			"include": "#strings-singlequote"
		},
		{
			"include": "#constants"
		},
		{
			"include": "#states"
		},
		{
			"include": "#properties"
		},
		{
			"include": "#functions"
		},
		{
			"include": "#frames"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "support.class.zscript",
					"match": "(?i)\\b(class\\s+[A-Z_]\\w*)\\b"
				},
				{
				"name": "keyword.control.zscript",
				"match": "(?i)\\b(switch|case|break|if|else|while|for|return|class|struct|goto|stop|loop|wait|states|default|let|is|fail|replaces)\\b"
			},
			{
				"name": "keyword.operator.zscript",
				"match": "(?i)\\b(\\+|==|=|<|>|<=|>=|~==|\\-|dot|cross|\\*|\\/)\\b"
			}]
		},
		"types": {
			"patterns": [{
				"name": "storage.type.zscript",
				"match": "(?i)\\b(BlockThingsIterator|CVar|enum|Array|fixed|void|bool|double|float|int|int8|int16|uint|uint8|uint16|Vector2|Vector3|String|Name|Sound|StateLabel|Color)\\b"
			},
			{
				"name": "storage.modifier.zscript",
				"match": "(?i)\\b(server|override|static|const|meta|out|in|version|virtual|native|ui|clearscope|virtualscope|play|property|prefix|action)\\b"
			}
			]
		},
        "comment": {
            "patterns": [
                {
                    "name": "comment.block.zscript",
                    "begin": "/\\*",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.definition.comment.begin.zscript"
                        }
                    },
                    "end": "\\*/",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.comment.end.zscript"
                        }
                    }
                },
                {
                    "begin": "//",
                    "beginCaptures": {
                        "0": {
                            "name": "punctuation.definition.comment.zscript"
                        }
                    },
                    "end": "(\n)",
                    "name": "comment.line.double-slash.zscript"
                }
            ]
        },
		"strings-doublequote": {
			"name": "string.quoted.double.zscript",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.zscript",
					"match": "\\\\."
				}
			]
		},
		"strings-singlequote": {
			"name": "string.quoted.single.zscript",
			"begin": "'",
			"end": "'",
			"patterns": [
				{
					"name": "constant.character.escape.zscript",
					"match": "\\\\."
				}
			]
		},
		"constants": {
			"patterns": [{
				"name": "constant.numeric.zscript",
				"match": "\\b(\\d+)\\b"
			},
			{
				"name": "constant.numeric.zscript",
				"match": "(?i)\\b(0x[\\dabcdef]+)\\b"
			},
			{
