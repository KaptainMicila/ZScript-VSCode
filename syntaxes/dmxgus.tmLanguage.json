{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "dmxgus",
    "patterns": [
        {
            "include": "source.mapinfolike#numbers"
        },
        {
            "name": "keyword.other.region.start.dmxgus",
            "match": "\\#region.*$"
        },
        {
            "name": "keyword.other.region.end.dmxgus",
            "match": "\\#endregion.*$"
        },
        {
            "name": "variable.parameter.dmxgus",
            "match": "\\b\\w+?\\b"
        }
    ],
    "scopeName": "source.dmxgus"
}
