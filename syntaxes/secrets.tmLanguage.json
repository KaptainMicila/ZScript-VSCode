{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "SECRETS",
    "patterns": [
        {
            "name": "keyword.control.secrets",
            "begin": "\\[",
            "end": "\\]",
            "contentName": "keyword.control.content.secrets"
        },
        {
            "name": "keyword.sector.secrets",
            "begin": "\\$",
            "end": ";",
            "contentName": "keyword.sector.location.secrets"
        },
        {
            "name": "keyword.other.region.start.secrets",
            "match": "\\#region.*$"
        },
        {
            "name": "keyword.other.region.end.secrets",
            "match": "\\#endregion.*$"
        }
    ],
    "scopeName": "source.secrets"
}
